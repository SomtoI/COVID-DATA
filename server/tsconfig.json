{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "lib": ["es6"],
      "outDir": "./dist",
      "rootDir": "./src",
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "strict": true,
      "allowJs": true,
      "types": ["node"]
    },
    "include": ["src"],
    "exclude": ["node_modules"]
  }
  